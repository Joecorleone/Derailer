<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="GameSignButton">
        <attr name="position" format="enum" >
            <enum name="start" value="0"/>
            <enum name="center" value="1"/>
            <enum name="end" value="2"/>
        </attr>
        <attr name="sign" format="reference"/>
        <attr name="shadow" format="reference"/>
        <attr name="sign_pressed" format="reference"/>
        <attr name="shadow_pressed" format="reference"/>
    </declare-styleable>
</resources>